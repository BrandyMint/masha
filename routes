                       Prefix Verb   URI Pattern                                                Controller#Action
                         root GET    /                                                          welcome#index
                              GET    /auth/:provider/callback(.:format)                         sessions#create
                              POST   /auth/:provider/callback(.:format)                         sessions#create
                      signout DELETE /signout(.:format)                                         sessions#destroy
                         user POST   /user(.:format)                                            user#create
                     new_user GET    /user/new(.:format)                                        user#new
                    edit_user GET    /user/edit(.:format)                                       user#edit
                              GET    /user(.:format)                                            user#show
                              PATCH  /user(.:format)                                            user#update
                              PUT    /user(.:format)                                            user#update
                              DELETE /user(.:format)                                            user#destroy
                     projects GET    /projects(.:format)                                        projects#index
                      project GET    /projects/:id(.:format)                                    projects#show
